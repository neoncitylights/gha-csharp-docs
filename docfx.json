{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"src/ChemistryNet/ChemistryNet.csproj"
					],
					"exclude": [
						"**/bin/**",
						"**/obj/**"
					],
					"src": "ChemistryNet/"
				}
			],
			"dest": "obj/api/ChemistryNet",
			"properties": {
				"TargetFramework": "netcoreapp3.1"
			}
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"**/*.yml"
				],
				"cwd": "obj/api/ChemistryNet",
				"dest": "doc/api/ChemistryNet"
			},
			{
				"files": [ "**.md" ],
				"cwd": "conceptual/ChemistryNet",
				"dest": "doc"
			},
			{
				"files": [
					"*.md",
					"dev/**.md",
					"toc.yml"
				]
			}
		],
		"dest": "live"
	}
}
